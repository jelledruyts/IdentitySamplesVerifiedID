{
  "attestations": {
    "idTokens": [
      {
        "mapping": [
          {
            "outputClaim": "userName",
            "required": true,
            "inputClaim": "email",
            "indexed": true
          },
          {
            "outputClaim": "name",
            "required": true,
            "inputClaim": "name",
            "indexed": false
          }
        ],
        "configuration": "https://login.microsoftonline.com/<tenant-id>/v2.0/.well-known/openid-configuration",
        "clientId": "<client-id>",
        "redirectUri": "vcclient://openid/",
        "scope": "openid profile email"
      }
    ]
  },
  "validityInterval": 2592000,
  "vc": {
    "type": [
      "Verified Staff"
    ]
  }
}